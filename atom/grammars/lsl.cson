    'number':
        'name': 'constant.numeric.lsl'
        'patterns': [
            {
                'match': '\\b(?i:(\\d+\\.\\d*(e[\\-\\+]?\\d+)?))(?=[^a-zA-Z_])'
                'name': 'constant.numeric.float.lsl'
            }
            {
