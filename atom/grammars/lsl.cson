                'match': '~'
            }
            {
                'name': 'keyword.operator.bit-wise.xor.lsl'
                'match': '\\^'
            }
            {
                'name': 'keyword.operator.bit-wise.and.lsl'
                'match': '&'
            }
            {
                'name': 'keyword.operator.bit-wise.or.lsl'
                'match': '\\|'
            }
            {
