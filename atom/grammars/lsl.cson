            }
            {
                'match': '\\b(?i:(\\d+e[\\-\\+]?\\d+))'
                'name': 'constant.numeric.float.lsl'
            }
            {
                'match': '\\b(?i:(0x\\h*))'
                'name': 'constant.numeric.integer.hexadecimal.lsl'
            }
            {
                'match': '\\b(0[0-7]+)'
                'name': 'constant.numeric.integer.octal.lsl'
            }
            {
